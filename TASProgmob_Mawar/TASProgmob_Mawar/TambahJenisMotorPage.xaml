<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TASProgmob_Mawar.TambahJenisMotorPage" Title="Tambah Jenis Motor">
  <StackLayout>
    <Label Text="Masukan Nama Merk :" />
    <Entry x:Name="txtNamaMerk" Placeholder="insert nama merk"/>
    <Label Text="Masukan Nama Jenis Motor :" />
    <Entry x:Name="txtNamaJenisMotor" Placeholder="insert nama jenis motor"/>
    <Button x:Name="btnTambahJenis" Text="Tambah" />
  </StackLayout>
</ContentPage>